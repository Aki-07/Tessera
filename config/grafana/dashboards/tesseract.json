        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.2 },
              { "color": "orange", "value": 0.4 },
              { "color": "red", "value": 0.7 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
