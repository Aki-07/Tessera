      "description": "Rate of battle rounds executed per second over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Rounds/sec",
            "axisPlacement": "auto",
