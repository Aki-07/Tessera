          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (tool, instance) (rate(capsule_tool_calls_total{job=~\"$job\"}[$__rate_interval]))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Tool Calls Detail",
      "transformations": [
